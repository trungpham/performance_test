      document.addEventListener('DOMContentLoaded', function () {
        var startTime = document.location.search.replace('?', ''),
            endTime = (new Date()).getTime(),
            timeInfoElement;
  
        timeInfoElement = document.createElement('strong');
        timeInfoElement.style.color = 'red';
        timeInfoElement.innerHTML = 'LOAD TIME FROM <u>SERVER</u>: ' + (endTime - startTime) + 'ms</strong>';
        document.body.insertBefore(timeInfoElement, document.body.childNodes[0]);
      }, false);
    <% end %>
  <% end %>
  
